<script>
    //Limpa os campos mesmo que eles tenham sito recuperados pelo request.
    try {
        $("#mudaReset").click(function () {
            document.getElementById("limpaNaMarra1").setAttribute("value", "");
            document.getElementById("limpaNaMarra2").setAttribute("value", "");
            document.getElementById("limpaNaMarra3").setAttribute("value", "");
            document.getElementById("ConsultaVeiculo").submit();
        });
    }
    catch (e) {
        alert(e);
    }

    try
    {
        function validateForm()
        {
            if (document.getElementsByName('tipo_cobranca')[0].value == "D") {
                if (document.getElementsByName('cda')[0].value == "") {
                    chamaErro7();
                    return false;
                }
            }

            if (
                document.getElementsByName('dta_cobranca')[0].value == "" ||
                document.getElementsByName('dta_pagamento_custas')[0].value == "" ||
                document.getElementsByName('valor_divida')[0].value == "" ||
                document.getElementsByName('valor_custas')[0].value == "" ||
                document.getElementsByName('valor_debito_total')[0].value == "" ||
                document.getElementsByName('ano_exercicio')[0].value == "" ||
                document.getElementsByName('uf_cobranca')[0].value == "" ||
                document.getElementsByName('tipo_cobranca')[0].value == ""
                )
            {

                $(document).ready(function () {
                    $("#msgErro5").modal();
                    return false;
                });
                return false;
            }
            else
            {
                document.getElementById("rd").value = true;
                document.getElementById("FormAltera").submit();
                return true;
            }
        }
    }
    catch(e)
    {
        alert(e);
    }
</script>

