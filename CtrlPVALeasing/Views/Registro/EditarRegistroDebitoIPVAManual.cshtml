<!-- O section Scripts é necessário para funcionar algumas mascaras -->
@section Scripts {
<script>
    function mudaDisableds() {
        if (document.getElementById("vdt") != null) {
            if (document.getElementById("vdt").value == 'C') {
                document.getElementById("dta_cobranca").removeAttribute('disabled');
                document.getElementById("uf_cobranca").removeAttribute('disabled');
                document.getElementById("valor_divida").removeAttribute('disabled');
                document.getElementById("ano_exercicio").removeAttribute('disabled');
                document.getElementById("cda").setAttribute("disabled", "disabled");
                document.getElementById("valor_custas").setAttribute("disabled", "disabled");
                document.getElementById("debito_protesto").setAttribute("disabled", "disabled");
                document.getElementById("nome_cartorio").setAttribute("disabled", "disabled");
                //document.getElementById("divida_ativa_serasa").setAttribute("disabled", "disabled");
                //document.getElementById("protesto_serasa").setAttribute("disabled", "disabled");
                document.getElementById("dta_custas").setAttribute("disabled", "disabled");
                document.getElementById("valor_debito_total_parc").value = document.getElementById("valor_divida").valor;

                document.getElementById("cda").value = null;
                document.getElementById("valor_custas").value = null;
                document.getElementById("debito_protesto").checked = false;
                document.getElementById("nome_cartorio").value = null;
                document.getElementById("dta_custas").value = null;
            }
            else if (document.getElementById("vdt").value == 'D') {
                document.getElementById("dta_cobranca").removeAttribute('disabled');
                document.getElementById("uf_cobranca").removeAttribute('disabled');
                document.getElementById("valor_divida").removeAttribute('disabled');
