            var resultado = vt.substring(0, vt.length - 2) + "," + vt.substring(vt.length - 2, vt.length);

            if (resultado.toString() != "NaN") {
                document.getElementsByName("valor_debito_total_parc")[0].value = resultado;
            }
        };

